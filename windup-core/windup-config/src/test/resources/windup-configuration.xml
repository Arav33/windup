<?xml version="1.0" encoding="UTF-8"?>
<c:configuration xmlns:c="http://windup.jboss.org/v1/configuration" xmlns:j="http://windup.jboss.org/v1/xml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<c:known-vendors>
		<vendor name="Apache">
			 <package-structure pattern="org.apache.*"/>
			 <namespace uri=""/>
		</vendor>
		
		<vendor name="Etc">
			<package-structure pattern="org.etc.*"/>
		</vendor>
	</c:known-vendors>
	
	
	</c:extensions type="java-source">
		<extension>.java</extension>
	</c:extensions>

	</c:extensions type="java-class">
		<extension>.class</extension>
	</c:extensions>
	
	</c:extensions type="xml">
		<extension>.xml</extension>
		<extension>.tld</extension>
	</c:extensions>
	
	</c:extensions type="zip">
		<extension>.ear</extension>
		<extension>.war</extension>
	</c:extensions>

	<c:java-configuration>
		<c:blacklist-patterns>
			<pattern></pattern>
		</c:blacklist-patterns>
	</c:java-configuration>
	
	
	<c:archive-configuration>
		
	</c:archive-configuration>
</c:configuration>