<?xml version="1.0" encoding="US-ASCII"?>
<xml-bindings xmlns="http://www.eclipse.org/eclipselink/xsds/persistence/oxm"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema"
	package-name="org.jboss.windup.engine.model.xml" xml-accessor-type="PUBLIC_MEMBER"
	xml-accessor-order="ALPHABETICAL" xml-mapping-metadata-complete="false"
	xml-name-transformer="example.NameGenerator" supported-versions="2.4">
	
	<xml-schema element-form-default="QUALIFIED">
		<xml-ns prefix="pom" namespace-uri="http://maven.apache.org/POM/4.0.0" />
	</xml-schema>
	
	<java-types>
		<java-type name="MavenPomModel">
			<xml-root-element name="project"/>
			<xml-type namespace="http://maven.apache.org/POM/4.0.0" />
			<java-attributes>
				<xml-element java-attribute="groupId"/>
				<xml-element java-attribute="artifactId" />
				<xml-element java-attribute="version" />
				<xml-element java-attribute="parent" xml-path="parent" type="org.jboss.windup.engine.model.xml.MavenPomModel.Parent"/>
				<xml-element java-attribute="dependencies" xml-path="dependencies/dependency" type="org.jboss.windup.engine.model.xml.MavenPomModel.Dependency"/>
			</java-attributes>
		</java-type>
	</java-types>
	
</xml-bindings>
